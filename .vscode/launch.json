    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "exam-cli",
            "cargo": {
                "args": [
                    "build",
                    "--bin=exam-cli",
                    "--package=exam-cli"
                ],
                "filter": {
                    "name": "exam-cli",
                    "kind": "bin"
                }
            },
            "args": [
                "--name=ttt",
                "--count=2",
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "exam-trait",
            "cargo": {
                "args": [
                    "build",
                    "--bin=exam-trait",
                    "--package=exam-trait"
                ],
                "filter": {
                    "name": "exam-trait",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "exam-struct",
            "cargo": {
                "args": [
                    "build",
                    "--bin=exam-struct",
                    "--package=exam-struct"
                ],
                "filter": {
                    "name": "exam-struct",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'exam-cli'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=exam-cli",
                    "--package=exam-cli"
                ],
                "filter": {
                    "name": "exam-cli",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}